/// ユーザー
///
/// @namespace 認証
model User {
  id Int @id @default(autoincrement()) @map("id")

  createdAt DateTime  @default(now()) @map("created_at")
  updatedAt DateTime  @default(now()) @updatedAt @map("updated_at")
  deletedAt DateTime? @map("deleted_at")

  bookmarks   Bookmark[]
  googleUsers GoogleUser[]
  passkeys    Passkey[]

  @@map("users")
}
