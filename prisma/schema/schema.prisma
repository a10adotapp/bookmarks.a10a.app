datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider        = "prisma-client-js"
  output          = "../generated/client"
  previewFeatures = ["prismaSchemaFolder"]
}

generator markdown {
  provider = "prisma-markdown"
  output   = "../../SCHEMA.md"
  title    = "Schema"
}
